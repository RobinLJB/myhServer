	.logo-xt img{padding-top: 12px;}
	.title{color: white; font-size: 16px;width: 67%;display: inline-block;height: 100%;     line-height: 56px;   border-top-right-radius: 5px;border-bottom-right-radius: 5px;}
	.android{margin-top: 11px;}
	.iphone .logo-xt{background-color: #368ad9;}
	.iphone .title{background-color: #4ea4f6;}
	.android .title{background-color: #94c136;}
	.android .logo-xt{background-color: #75b139;}
	*,h1,h2,h3,h4,h5,h6,ul,label,select{margin:0; padding:0;}
    a:link,a:active,a:visited,a:hover{text-decoration:none;}
    h1,h2,h3,h4,h5,h6{font-weight: normal;}
    a img{border:0;}
    li{list-style-type:none;}
    button,select{border:none;}
    body{
        background:white;
        width: 100%;
    }
    input[type="number"]{-moz-appearance:textfield;}
    .me a,.scan_tow button,.scan_four ul li,.hint button,.scan_four ul li button,.lb_password ul li input,.gross_1,.lb_password button,.Investment a,.header_t{behavior: url(ie-css3.htc);}
    *,h1,h2,h3,h4,h5,h6,ul,label,select{margin:0; padding:0;}
    a:link,a:active,a:visited,a:hover{text-decoration:none;}
    h1,h2,h3,h4,h5,h6{font-weight: normal;}
    a img{border:0;}
    li{list-style-type:none;}
    button,select{border:none;}
    body{
        background:white;
        width: 100%;
    }
    input[type="number"]{-moz-appearance:textfield;}
    .me a,.scan_tow button,.scan_four ul li,.hint button,.scan_four ul li button,.lb_password ul li input,.gross_1,.lb_password button,.Investment a,.header_t{behavior: url(ie-css3.htc);}


    /*APP下载开始*/
    .dow{
        background: url("/asset/front/images/back_1.png") no-repeat center;
        background-size: cover;
        overflow: hidden;
    }
    .download_box{
        overflow: hidden;
        width: 1200px;
        margin: 0 auto;
        padding: 140px 0 30px 50px;
    }
    .dow_img{
        float: left;
    }
    .dow_img img{
        display: block;
    }
    .dow_r{
        float: left;
        margin-left: 130px;
    }
    .dr_t h2{
        display: block;
        color: white;
        font-size: 36px;
        font-weight: normal;
        margin-top: 74px;
    }
    .dr_t span{
        display: block;
        color: white;
        font-size: 20px;
        margin-top: 40px;
    }
    .dr_t img{
        display: block;
        margin-top: 26px;
    }
    .dr_b{
        margin-top: 76px;
    }
    .drb_b{
        overflow: hidden;
    }
    .drb_b button{
        float: left;
        width: 220px;
        height: 60px;
        border: solid 2px #e9e9e9;
        border-radius: 8px;
        color: white;
        background: none;
        font-size: 20px;
        line-height: 56px;
        margin-right: 26px;
        padding-left: 84px;
        text-align: left;
    }
    .drb_b > .b_1{
        background: url("/asset/front/images/dow_iphone.png") no-repeat 26px 10px;
    }
    .drb_b > .b_2{
        background: url("/asset/front/images/dow_Android.png") no-repeat 26px 10px;
    }
    .dr_b img{
        display: block;
        margin:86px auto;
        margin-top: 86px;
        width: 180px;
        height: 180px;
    }/*APP下载结束*/
    /*媒体频道开始*/
    .media_channel{
        position: relative;
        width: 1200px;
        margin: 192px auto 0 auto;
        overflow: hidden;
    }
    .mc_l{
        float: left;
    }
    .mcl_img{
        width:860px;
        height: 320px;
    }
    .mc_content{
        width: 860px;
        background: white;
        margin-top: 20px;
    }
    .mcc_title{
        padding: 26px 0 30px 22px;
        border-bottom: 2px solid #f4f4f4;
    }
    .mcc_title h2{
        color: #3c3c3c;
        font-size: 24px;
    }
    .mcc_content{
        padding: 30px 38px 20px 22px;
        border-bottom: 1px solid #f4f4f4;
    }
    .mcc_content h3{
        color: #3c3c3c;
        font-size: 16px;
    }
    .mcc_content span{
        display: block;
        margin-top: 24px;
        color: #909090;
        font-size: 14px;
        overflow:hidden;
        text-overflow:ellipsis;
        display:-webkit-box;
        -webkit-box-orient:vertical;
        -webkit-line-clamp:2;
    }
    .mcc_foot{
        overflow: hidden;
        margin-top: 24px;
    }
    .mcf_l{
        float: left;
    }
    .mcf_l ul{
        overflow: hidden;
    }
    .mcf_l ul li{
        float: left;
    }
    .quantity{
        color: #909090;
        margin-left: 5px;
        margin-right: 26px;
    }
    .mcf_r{
        float: right;
    }
    .mcf_time{
        color: #909090;
    }
    .mc_r{
        float: right;
        width: 300px;
    }
    .mcr_box{
        background: white;
    }
    .mcr_wz{
        padding:5px 26px 0 20px;
    }
    .mcr_box h3{
        padding: 20px 0 0 20px;
        color: #909090;
        font-size: 18px;
    }
    .mcr_chunk{
        padding: 20px 0 20px 0;
        border-bottom: 1px solid #e9e9e9;
    }
    .mcr_chunk a{
        color: #3c3c3c;
        font-size: 14px;
        display: block;
        overflow:hidden;
        text-overflow:ellipsis;
        display:-webkit-box;
        -webkit-box-orient:vertical;
        -webkit-line-clamp:2;
    }
    .mcr_chunk span{
        display: block;
        color: #909090;
        font-size: 12px;
        margin-top: 14px;
    }/*媒体频道结束*/

    /*文章详情开始*/
    .mc_article{
        width: 860px;
        background: white;
        padding: 50px 60px 0 56px;
    }
    .mc_text h2{
        color: #3c3c3c;
        font-size: 24px;
        text-align: center;
    }
    .mc_text span{
        display: block;
        color: #909090;
        font-size: 12px;
        text-align: center;
        margin-top: 22px;
    }
    .mc_text a{
        color: #3c3c3c;
        font-size: 14px;
        line-height: 2.5;
        display: block;
        margin-top: 42px;
    }
    .mc_pl{
        margin-top: 40px;
        overflow: hidden;
    }
    .mc_pl h3{
        font-size: 18px;
        color: #3c3c3c;
    }
    .mc_pl h3 a{
        color: #fc5144;
    }
    .mc_pl textarea{
        padding: 26px 16px 10px 16px;
        border:none;
        background: #f4f4f4;
        width: 740px;
        height: 118px;
        color: #3c3c3c;
        font-size: 14px;
        resize: none;
        outline:none;
        margin-top: 16px;
    }
    .mcp_btn{
        float: right;
        margin-top: 8px;
    }
    .mcp_btn input{
        text-align: center;
        line-height: 32px;
        width: 85px;
        height: 32px;
        background: #71b8ff;
        border-radius: 4px;
        color: white;
        font-size: 14px;
        border:none;
        outline:none;
     }
    .mcp_btn button{
        text-align: center;
        line-height: 32px;
        width: 85px;
        height: 32px;
        background: #fc5144;
        border-radius: 4px;
        color: white;
        font-size: 14px;
        border:none;
        outline:none;
        margin-left: 14px;
    }
    .critical_box{
        padding-bottom: 80px;
    }
    .cb_box{
        border-bottom: 1px solid #f4f4f4;
        padding-bottom: 32px;
        margin-top: 38px;
    }
    .critical_top{
        overflow: hidden;
    }
    .cr_left{
        float: left;
        overflow: hidden;
    }
    .cr_left img{
        float: left;
        width: 48px;
        height: 48px;
        border-radius: 50px;
    }
    .cr_name{
        float: left;
        margin-top: 4px;
        margin-left: 16px;
    }
    .cr_name span{
        font-size: 14px;
        color: #909090;
        display: block;
    }
    .cr_name i{
        font-style: normal;
        font-size: 12px;
        color: #909090;
    }
    .cr_right{
        float: right;
        margin-top: 16px;
    }
    .cr_right span{
        color: #fc5144;
        font-size: 14px;
    }
    .cr_right span a{
        cursor: pointer;
        color: #a6a6a6;
    }
    .critical_bottom{
        margin-top: 26px;
        padding-left: 64px;
    }
    .critical_bottom span{
        color: #3c3c3c;
        font-size: 14px;
    }
    .critical_reply{
        display: none;
        background: white;
        border: 1px solid #e9e9e9;
        height: 60px;
        margin-top: 32px;
    }
    .critical_reply textarea{
        float: left;
        border: none;
        width: 630px;
        height: 56px;
        resize: none;
        outline:none;
        padding: 10px;
        overflow: hidden;
        clip:rect();
    }
    .critical_reply button{
        margin-top: 14px;
        margin-right: 12px;
        line-height: 32px;
        width: 84px;
        height: 32px;
        color: white;
        font-size: 14px;
        background: #bdbdbd;
        border-radius: 4px;
        float: right;
    }/*文章详情结束*/

    /*关于我们开始*/
    .about_box{
        overflow: hidden;
        width: 1200px;
        margin:192px auto 80px auto;
    }
    .ab_left{
        float: left;
    }
    .ab_left span{
        display: block;
        width: 230px;
        height: 80px;
        background: #fc5144;
        color: white;
        font-size: 24px;
        line-height: 80px;
        padding-left: 10px;
        text-align: center;
    }
    .ab_left i{
        position: absolute;
        width: 0;
        height: 0;
        border-top: 10px solid #db3124;
        border-left: 10px solid transparent;
    }
    .ab_left ul{
        margin-left: 10px;
    }
    .ab_left ul li{
        color: #494949;
        font-size: 18px;
        width: 220px;
        text-align: center;
        margin-bottom: 1px;
        background: white;
    }
    .ab_left ul li a{
        display: block;
        width: 220px;
        padding: 22px 0;
        color: #494949;
    }
    .on_hove{
        border-left: 4px solid #fc5144;
    }
    .ab_left ul li a:hover{
        background: #d49898;
        color: white;
    }
    .ab_right{
        float: right;
    }
    .text_introduce{
        width: 940px;
        background: white;
        padding: 28px 62px 90px 62px;
    }
    .text_introduce h2{
        color: #3c3c3c;
        font-size: 24px;
    }
    .text_introduce span{
        display: block;
        margin-top: 48px;
        line-height: 4;
        color: #3c3c3c;
        font-size: 14px;
    }
    .text_none{
        display: none;
    }
    /*关于我们结束*/
    .nb_name a{
        color: #909090;
        font-size: 14px;
        float: right;
        line-height: 26px;
        padding-right: 55px;
        display:table-cell;      /*按照单元格的样式显示元素*/
        vertical-align:bottom;   /*底对齐*/
    }
    .nbn_l a{
        padding: 0;
    }
</style>
</@override>
