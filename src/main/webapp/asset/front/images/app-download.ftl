<@override name="head">
	<style>
	.container {width: 1200px;margin: 0 auto;}
	.down-main{background-color: #eef4fd;}   
	.down-main .col-md-6{margin: 50px 0;}
	.code{margin-top: 33px;}
	.text-left2{float: left;text-align: center;margin-left: 20px;height: 128px;overflow: hidden;}
	.text-float{float: left;} 
	.text-left2 div{height: 58px; line-height: 58px;width: 222px;overflow: hidden;    margin-top: 32px;}
	.text-float img{height: 128px; width: 128px;}  
